{
  "name": "vue-poker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run write-build-info && vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "r-dev": "concurrently \"../rust/run_watch.sh\" \"vite\"",
    "r-build": "cd ../rust && wasm-pack build --dev --target web --out-dir ../../vue-poker/pkg range",
    "r1-build-release": "cd ../rust && wasm-pack build --release --target web --out-dir ../../vue-poker/pkg range",
    "r2-build-release": "cd ../rust && wasm-pack build --release --target web --out-dir ../../vue-poker/pkg poker_eval",
    "write-build-info": "node ../dev/writeBuildInfo.js"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.18",
    "@vueform/toggle": "^2.1.4",
    "@vueuse/core": "^10.7.0",
    "comlink": "^4.4.1",
    "dexie": "^3.2.4",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.3.10",
    "vue-tippy": "^6.3.1"
  },
  "devDependencies": {
    "@babel/types": "^7.23.6",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.19.2",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "date-fns": "^3.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.2.0",
    "vite": "^5.0.5",
    "vite-plugin-wasm": "^3.3.0",
    "vue-tsc": "^1.8.25"
  }
}
